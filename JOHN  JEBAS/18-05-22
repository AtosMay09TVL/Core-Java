package org.data;

public class StarPattern {
public static void main(String[] args) {
	String s ="johnjebas";
	String[] spl = s.split("");
	int k=0;
	for (int i = 0; i < spl.length; i++) 
		//System.out.println(i);
    { 
        
        for (int j = 0; j<i; j++){
        	
        	if (k<s.length()) {
        	 System.out.print(spl[k]);
             k++;
        }
			
		}
       
     
	System.out.println();
}
    }
}
