package basic;

/**
 * @author syntel
 *
 */
public class Array2Ex {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s="Angeetha";
        String[] spl=s.split("");
		int k=0;
		for(int i=0; i<spl.length;i++)
		{
			for(int j=0;j<i;j++)
		{
			System.out.println(spl[k]);
			++k;
		}
		System.out.println();
	}
	}
}
