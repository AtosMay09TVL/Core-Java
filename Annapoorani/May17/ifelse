package basic;

public class ifelse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
    String village="Thachanallur";

    if(village=="Thachanallur")
    {
    	System.out.println("error");
    }
    else if(village=="junction")
    {
    	System.out.println("nothing");
    }
    else
    {
    	System.out.println(" mismatched place");
    }

	}

}
